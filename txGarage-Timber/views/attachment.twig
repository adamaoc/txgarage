<div class="hero-wrap hero-wrap-default insta-box">
	<div class="insta-box__wrapper">
		<div id="instafeed"></div>
		<div class="insta-box__default"></div>
	</div>
	<h1 class="tab-color">{{post.post_title}}</h1>
</div>
<article class="{{ post.class }}">
  <img src="{{post.guid}}" alt="{{post._wp_attachment_image_alt}}" />
  <p class="attachment-desc">{{post.post_content}}</p>
</article>
